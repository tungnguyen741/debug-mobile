{
    "version": 2,
    "name":"debug-mobile",
    "builds": [
        { "src": "*.html", "use": "@vercel/static" },
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/index.js"
        }
    ]
}